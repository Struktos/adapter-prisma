{
  "name": "@struktos/prisma",
  "version": "0.1.0",
  "description": "Prisma adapter for Struktos.js - Unit of Work and repository implementations using Prisma Client",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./unit-of-work": {
      "import": "./dist/unit-of-work/index.mjs",
      "require": "./dist/unit-of-work/index.js",
      "types": "./dist/unit-of-work/index.d.ts"
    },
    "./repository": {
      "import": "./dist/repository/index.mjs",
      "require": "./dist/repository/index.js",
      "types": "./dist/repository/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.mjs",
      "require": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "unit-of-work": ["dist/unit-of-work/index.d.ts"],
      "repository": ["dist/repository/index.d.ts"],
      "types": ["dist/types/index.d.ts"],
      "errors": ["dist/errors/index.d.ts"]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc --out docs src/index.ts",
    "prepublishOnly": "npm run build",
    "release:patch": "./scripts/publish.sh patch",
    "release:minor": "./scripts/publish.sh minor",
    "release:major": "./scripts/publish.sh major",
    "release": "./scripts/publish.sh",
    "push": "./scripts/push.sh"
  },
  "keywords": [
    "struktos",
    "prisma",
    "unit-of-work",
    "repository",
    "transaction",
    "orm",
    "database",
    "typescript",
    "nodejs",
    "enterprise"
  ],
  "author": "Struktos Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/struktos/prisma.git",
    "directory": "packages/prisma"
  },
  "bugs": {
    "url": "https://github.com/struktos/prisma/issues"
  },
  "homepage": "https://struktos.dev",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0",
    "@struktos/core": "^0.1.1",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.10.0",
    "@struktos/core": "^0.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  }
}